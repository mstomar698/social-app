{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "meta-charset": "off",
    "meta-charset-utf-8": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "button-type": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/paint": "off",
    "axe/structure": "off",
    "axe/parsing": "off"
  }
}